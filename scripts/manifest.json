{
    "name": "AdaptAble",
    "description": "Use AI to adapt to each user for more accessible websites",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "main.html",
      "default_icon": "images/icon.png",
      "default_width": 340,
      "default_height": 300
    },
    "content_scripts": [ { "matches": ["*://*.wikipedia.org/*"], "js": ["dbInit.js", "tf.min.js", "content.js"] } ]
  }